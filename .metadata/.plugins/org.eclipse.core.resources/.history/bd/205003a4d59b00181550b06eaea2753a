from tkinter import *

root = Tk()
root.geometry("640x480")

bttn1 = Button(root, text="Kamera")
bttn1.config(height=5, width=20)
bttn1.pack()
bttn2 = Button(root, text="Sensoren")
bttn2.config(height=5, width=20)
bttn2.pack()
bttn3 = Button(root, text="RFID")
bttn3.config(height=5, width=20)
bttn3.pack()
bttn4 = Button(root, text="Raspberry")
bttn4.config(height=5, width=20)
bttn4.pack()
bttn5 = Button(root, text="Akkustand")
bttn5.config(height=5, width=20)
bttn5.pack()

root.mainloop()